{
  "name": "i18n-check-keys",
  "version": "0.1.7",
  "description": "A tool to check for missing internationalization keys in a project",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/heimeiyaodagongzai/i18n-check-keys"
  },
  "keywords": [
    "i18n",
    "check keys",
    "国际化检查"
  ],
  "author": "",
  "scripts": {
    "test": "node test/index.esm.mjs",
    "testCjs": "node test/index.cjs",
    "build": "tsc --declaration && rollup -c"
  },
  "dependencies": {
    "@babel/parser": "^7.22.4",
    "@babel/traverse": "^7.22.4",
    "@babel/types": "^7.22.4",
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.1",
    "@rollup/plugin-typescript": "^11.1.1",
    "@types/babel__traverse": "^7.20.1",
    "@types/node": "^20.3.1",
    "@types/webpack": "^4.41.26",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.3",
    "typescript": "^5.1.3"
  },
  "license": "ISC"
}
